# Environment Variables Template for AWS Elastic Beanstalk
# Copy this to .env for local development or set these in EB environment

# Security
SECRET_KEY=your-secret-key-here
DEBUG=False

# Allowed Hosts (comma-separated)
ALLOWED_HOSTS=your-eb-url.elasticbeanstalk.com,yourdomain.com

# Database (Elastic Beanstalk will provide this automatically if you add RDS)
# DATABASE_URL=postgres://username:password@host:port/database_name

# AWS Settings (optional, for S3 static files)
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_STORAGE_BUCKET_NAME=your-bucket-name
# AWS_S3_REGION_NAME=us-east-1
